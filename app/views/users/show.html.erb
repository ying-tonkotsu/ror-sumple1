<div class="main user-show">
  <div class="container">
    <div class="user">
    <%# プロフィール画像 %>
    <img src="<%= "/user_images/#{@user.image_name}" %>">
    <%# ユーザー名 %>
      <h2><%= @user.name %></h2>
    <%# メールアドレス %>
      <p><%= @user.email %></p>
    </div>
    <%= link_to("edit","/users/#{@user.id}/edit")%>
  </div>
</div>